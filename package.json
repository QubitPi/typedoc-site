  "author": "Gerrit Birkeland <gerrit@gerritbirkeland.com>",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "eleventy-sass": "^2.1.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "node-fetch": "^3.2.10",
    "semver": "^7.3.8",
    "ts-node": "^10.9.1"
